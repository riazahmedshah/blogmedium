{
  "compilerOptions": {
    "jsx": "react-jsx",  
    "jsxImportSource": "react",  
    "allowJs": true, 
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "resolveJsonModule": true,  
    "esModuleInterop": true      
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
